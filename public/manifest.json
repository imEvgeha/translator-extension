{
    "name": "__MSG_name__",
    "description": "__MSG_desc__",
    "default_locale": "en",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "16": "favicon.png",
        "32": "logo192.png",
        "48": "logo192.png",
        "128": "logo512.png"
    },
    "action": {
        "default_title": "__MSG_title__",
        "default_icon": {
            "16": "favicon.png",
            "32": "logo192.png",
            "48": "logo192.png",
            "128": "logo512.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "tabs",
        "activeTab",
        "bookmarks",
        "storage",
        "management"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
}
